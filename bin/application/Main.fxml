<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPane" maxWidth="800.0" prefHeight="480.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controller">
   <children>
      <Canvas fx:id="canvas" height="480.0" layoutY="66.0" width="800.0" />
      <Button fx:id="buttonStart" layoutX="375.0" layoutY="30.0" mnemonicParsing="false" onMouseClicked="#setButtonAction" text="START" />
      <TextField fx:id="txField" layoutX="37.0" layoutY="30.0" prefHeight="25.0" prefWidth="95.0" />
      <Label fx:id="label" layoutX="20.0" layoutY="14.0" prefHeight="17.0" prefWidth="149.0" text="ENTER NUMBER OF RANDOM SEEDS" textAlignment="CENTER">
         <font>
            <Font size="8.0" />
         </font>
      </Label>
      <Button layoutX="145.0" layoutY="34.0" mnemonicParsing="false" onAction="#getTxtFeed" text="GENERATE">
         <font>
            <Font size="8.0" />
         </font>
      </Button>
      <Label fx:id="genLab" alignment="TOP_LEFT" layoutX="538.0" layoutY="23.0" prefHeight="39.0" prefWidth="172.0">
         <font>
            <Font size="8.0" />
         </font>
      </Label>
      <Button fx:id="buttonStop" layoutX="324.0" layoutY="30.0" mnemonicParsing="false" onMouseClicked="#stopSim" prefHeight="25.0" prefWidth="51.0" text="STOP" visible="false" />
      <Button fx:id="buttonPause" layoutX="425.0" layoutY="30.0" mnemonicParsing="false" onMouseClicked="#pauseSim" text="PAUSE" visible="false" />
   </children>
</AnchorPane>
